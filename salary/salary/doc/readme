CREATE TABLE `NewTable` (
`ID`  int(11) NOT NULL AUTO_INCREMENT COMMENT '自增长主键' ,
`WORKER_NUM`  varchar(50) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL COMMENT '员工编码' ,
`NAME`  varchar(50) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT '员工姓名' ,
`SALARY_DETAILS`  text CHARACTER SET utf8 COLLATE utf8_general_ci NULL COMMENT '工资细节' ,
`CREATE_TIME`  timestamp NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间' ,
`ORG_ID`  int(11) NULL DEFAULT NULL COMMENT '机构id' ,
`ORG_TYPE`  int(11) NULL DEFAULT NULL COMMENT '机构类型' ,
`YEAR_MONTH_ID`  varchar(11) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT '与excel项目表对应' ,
`PERSON_ID`  varchar(50) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL ,
PRIMARY KEY (`ID`)
)
ENGINE=InnoDB
DEFAULT CHARACTER SET=utf8 COLLATE=utf8_general_ci
AUTO_INCREMENT=1
ROW_FORMAT=COMPACT
;
CREATE TABLE `NewTable` (
`ID`  int(11) NOT NULL AUTO_INCREMENT ,
`TITLES`  text CHARACTER SET utf8 COLLATE utf8_general_ci NULL COMMENT 'excel项目名' ,
`CREATE_TIME`  timestamp NULL DEFAULT CURRENT_TIMESTAMP ,
`YEAR_MONTH_ID`  int(11) NULL DEFAULT NULL ,
`ORG_ID`  int(11) NULL DEFAULT NULL ,
`ORG_TYPE`  int(11) NULL DEFAULT NULL ,
PRIMARY KEY (`ID`)
)
ENGINE=InnoDB
DEFAULT CHARACTER SET=utf8 COLLATE=utf8_general_ci
AUTO_INCREMENT=1
ROW_FORMAT=COMPACT
;

